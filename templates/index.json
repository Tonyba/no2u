                  },
                  "block_order": [
                    "text_zcAym3"
                  ]
                },
                "accordion_row_Rbeq3m": {
                  "type": "_accordion-row",
                  "name": "t:names.accordion_row",
                  "settings": {
                    "heading": "How do NO2U products help boost Nitric Oxide levels?",
                    "open_by_default": false,
                    "icon": "none",
                    "width": 20
                  },
                  "blocks": {
                    "text_hYU9pi": {
                      "type": "text",
                      "settings": {
                        "css_class": "",
                        "text": "<p>NO2U products are scientifically formulated to restore and enhance Nitric Oxide production in the body. Developed by Dr. Nathan S. Bryan, a leading expert in Nitric Oxide research, these supplements are designed to support cardiovascular health, improve circulation, and provide antioxidant benefits.</p>",
                        "width": "100%",
                        "max_width": "normal",
                        "alignment": "left",
                        "type_preset": "rte",
                        "font": "var(--font-body--family)",
