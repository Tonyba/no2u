                "accordion_row_H87fzM": {
                  "type": "_accordion-row",
                  "name": "t:names.accordion_row",
                  "settings": {
                    "heading": "What products does NO2U offer?",
                    "open_by_default": false,
                    "icon": "none",
                    "width": 20
                  },
                  "blocks": {
                    "text_jVUHn6": {
                      "type": "text",
                      "settings": {
                        "css_class": "",
                        "text": "<p>NO2U offers a range of Nitric Oxide supplements, including:</p><ul><li><strong>NO2U Nitric Oxide Lozenges</strong>: Convenient lozenges to boost N.O. levels.</li><li><strong>N.O.Beetz</strong>: A fermented oxalate free beet-based supplement for enhanced Nitric Oxide production.</li><li><strong>Power & Performance Stack</strong>: A combination pack for comprehensive support.</li></ul>",
                        "width": "100%",
                        "max_width": "normal",
                        "alignment": "left",
                        "type_preset": "rte",
                        "font": "var(--font-body--family)",
                        "font_size": "1rem",
                        "line_height": "normal",
                        "letter_spacing": "normal",
                        "case": "none",
                        "wrap": "pretty",
                        "color": "var(--color-foreground)",
                        "background": false,
                        "background_color": "#00000026",
                        "corner_radius": 0,
                        "padding-block-start": 0,
